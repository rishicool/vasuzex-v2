{
  "name": "vasuzex",
  "version": "2.1.0",
  "description": "Laravel-inspired framework for Node.js monorepos - V2 with optimized dependencies",
  "type": "module",
  "main": "./framework/index.js",
  "exports": {
    ".": "./framework/index.js",
    "./Foundation": "./framework/Foundation/index.js",
    "./Foundation/*": "./framework/Foundation/*.js",
    "./Database": "./framework/Database/index.js",
    "./Database/*": "./framework/Database/*.js",
    "./Http": "./framework/Http/index.js",
    "./Http/*": "./framework/Http/*.js",
    "./Support": "./framework/Support/index.js",
    "./Support/*": "./framework/Support/*.js",
    "./Support/Facades": "./framework/Support/Facades/index.js",
    "./Support/Facades/*": "./framework/Support/Facades/*.js",
    "./Services/*": "./framework/Services/*/index.js",
    "./client": "./frontend/client/index.js",
    "./client/*": "./frontend/client/*.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "create-vasuzex": "bin/create-vasuzex.js",
    "vasuzex": "framework/Console/cli.js"
  },
  "workspaces": [
    "apps/**/api",
    "apps/**/web",
    "apps/media-server",
    "frontend/client"
  ],
  "imports": {
    "vasuzex": "./framework/index.js",
    "vasuzex/*": "./framework/*",
    "#framework": "./framework/index.js",
    "#framework/*": "./framework/*",
    "#database": "./database/index.js",
    "#database/*": "./database/*",
    "#models": "./database/models/index.js",
    "#models/*": "./database/models/*",
    "#config": "./config/index.cjs",
    "#config/*": "./config/*"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "db:migrate": "npx vasuzex migrate",
    "test:all": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest tests/unit",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules npx jest tests/integration",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules npx jest tests/e2e",
    "scan:issues": "node deep-scan-issues.cjs",
    "dev:pilot-test-api": "turbo run dev --filter=pilot-test-api",
    "start:pilot-test-api": "turbo run start --filter=pilot-test-api",
    "dev:pilot-test-web": "turbo run dev --filter=pilot-test-web",
    "start:pilot-test-web": "turbo run start --filter=pilot-test-web",
    "dev:integration-test-api": "turbo run dev --filter=integration-test-api",
    "start:integration-test-api": "turbo run start --filter=integration-test-api",
    "dev:test-gen-api-api": "turbo run dev --filter=test-gen-api-api",
    "start:test-gen-api-api": "turbo run start --filter=test-gen-api-api",
    "dev:cleanup-test-api": "turbo run dev --filter=cleanup-test-api",
    "start:cleanup-test-api": "turbo run start --filter=cleanup-test-api",
    "dev:auth-integration-test-api": "turbo run dev --filter=auth-integration-test-api",
    "start:auth-integration-test-api": "turbo run start --filter=auth-integration-test-api",
    "dev:e2e-test-api": "turbo run dev --filter=e2e-test-api",
    "start:e2e-test-api": "turbo run start --filter=e2e-test-api"
  },
  "keywords": [
    "framework",
    "laravel",
    "monorepo",
    "nodejs",
    "hybrid-dependencies"
  ],
  "author": "Vasuzex Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fs-extra": "^11.3.2",
    "guruorm": "^2.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^9.3.8",
    "ip2location-nodejs": "^9.7.0",
    "joi": "^17.13.3",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "maxmind": "^5.0.1",
    "multer": "^2.0.2",
    "node-plop": "^0.32.3",
    "ora": "^8.2.0",
    "pg": "^8.16.3",
    "plop": "^4.0.1",
    "razorpay": "^2.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.28.0",
    "redux-persist": "^6.0.0",
    "sharp": "^0.33.5",
    "stripe": "^20.0.0",
    "svelte": "^4.2.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.0.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "turbo": "^2.6.1",
    "vite": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "express": "^5.2.1",
      "react": "^18.2.0",
      "vue": "^3.4.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rishicool/vasuzex.git"
  },
  "bugs": {
    "url": "https://github.com/rishicool/vasuzex/issues"
  },
  "homepage": "https://github.com/rishicool/vasuzex#readme",
  "packageManager": "pnpm@10.0.0"
}
